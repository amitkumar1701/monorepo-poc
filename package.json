{
  "name": "solar-poc",
  "version": "1.0.0",
  "description": "Multiple api in monorepo with lerna and yarn workspaces",
  "repository": {
    "type": "git",
    "url": "https://github.com/skrunchtech/monorepo-poc"
  },
  "author": "Amit Kumar",
  "scripts": {
    "docker:roofgraf": "lerna run --scope roofgraf docker",
    "docker:roofgraf:stop": "lerna run --scope roofgraf docker-stop",
    "dev:roofgraf": "lerna run --scope roofgraf dev",
    "dev:solargraf": "lerna run --scope solargraf dev",
    "start:roofgraf": "lerna run --scope roofgraf start",
    "start:solargraf": "lerna run --scope solargraf start",
    "docker-roofgraf": "docker-compose -f docker-compose-dev.yaml up roofgraf_api"
  },
  "license": "ISC",
  "private": true,
  "workspaces": ["packages/*"],
  "devDependencies": {
    "lerna": "^3.3.2"
  }
}